		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jun.biz.manager.dao.ProductDetailDao">

	<select id="selectByProductId" resultType="com.jun.biz.manager.model.ProductDetail">
